<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.keycloak.mapper.CorpMapper">
    <select id="selectItgCorp" resultType="com.example.keycloak.model.corp.CorpVo">
        SELECT A.CORP_CD
             , A.CORP_NM
             , A.CORP_REG_NO
             , A.CEO_NM
          FROM ITG_CORP A
         WHERE A.CORP_CD = #{corpCd}
    </select>
</mapper>